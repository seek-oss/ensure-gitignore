{
  "name": "ensure-gitignore",
  "version": "1.1.3",
  "description": "Ensure the presence of patterns within a project's gitignore",
  "main": "./lib-commonjs/index.js",
  "module": "./lib-es2015/index.js",
  "types": "./lib-types/index.d.ts",
  "files": [
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "lib*/**/*.json"
  ],
  "scripts": {
    "build": "skuba build-package",
    "format": "skuba format",
    "lint": "skuba lint",
    "release": "yarn build && changeset publish",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/ensure-gitignore.git"
  },
  "resolutions": {
    "**/@jest/types/@types/node": "^14.17.9"
  },
  "author": "SEEK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seek-oss/ensure-gitignore/issues"
  },
  "homepage": "https://github.com/seek-oss/ensure-gitignore#readme",
  "devDependencies": {
    "@changesets/cli": "^2.17.0",
    "@types/node": "^14.17.9",
    "skuba": "3.14.4"
  },
  "dependencies": {},
  "skuba": {
    "entryPoint": "src/index.ts",
    "template": "oss-npm-package",
    "type": "package",
    "version": "3.14.4"
  }
}
